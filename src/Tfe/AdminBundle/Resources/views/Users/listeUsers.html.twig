
{% extends "TfeCoreBundle:Default:index.html.twig" %}

{% block body %}
    <h2>Futur liste des utilisateurs</h2>

    <table class="table table-hover">
        <thead>
        <tr>
            <th>Pseudo</th>
            <th>Voir Profil</th>

        </tr>
        </thead>
        <tbody>
        {% for user in users %}
        <tr>
            <td>{{ user.username }}</td>
            <td>
                <a class="btn btn-primary" href="{{ path('tfe_admin_user_profil', {'id': user.id}) }}">Profil</a>
            </td>


        </tr>
        </tbody>
        <tbody>
        {% endfor %}

    </table>
{% endblock %}